{
  "name": "tiny-ci-github-integration",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon",
    "build": "tsc -p tsconfig.json",
    "lint": "eslint --format=codeframe '**/*.ts'"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "UseExpress=1 node --inspect=5657 -r ts-node/register ./src/server.ts",
    "ext": "ts"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Caian R. Ertl <hi@caian.org>",
  "license": "ISC",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.51",
    "@types/express": "^4.17.6",
    "@types/node": "^13.13.2",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "body-parser": "^1.19.0",
    "eslint": "^6.8.0",
    "express": "^4.17.1",
    "nodemon": "^2.0.3",
    "ts-node": "^8.9.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "jsonschema": "^1.2.6"
  }
}
